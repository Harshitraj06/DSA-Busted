 int doUnion(int a[], int n, int b[], int m)  {
        
        // Create a set to store the values of a[] and b[]
        set<int> s;
        int count = 0;
        for (int i=0; i<n; i++){
             // insert the elements of a[] in set.
            s.insert(a[i]);
        }
            for (int j=0; j<m; j++){
             // insert the elements of b[] in set.
            s.insert(b[j]);
            
        }
        return s.size();
}
