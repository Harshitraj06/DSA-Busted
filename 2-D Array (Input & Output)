#include <iostream>
using namespace std;
int main() {
   // Initilizing the 2-D array
   int arr[3][4];
   cout << "Enter the number to take input:";
   
   // Taking Input (Row-Wise)
   for(int i=0; i<3; i++){
       for(int j=0; j<4; j++){
           cin >> arr[i][j];
       }
    }
    
     // Taking Input (Column-Wise)
    for(int i=0; i<4; i++){
       for(int j=0; j<3; j++){
           cin >> arr[i][j];
       }
    }
    
    // For Printing
    for(int i=0; i<3; i++){
       for(int j=0; j<4; j++){
           cout << arr[i][j] << " ";
       }
       cout << endl;
    }
   return 0;
}
